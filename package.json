{
  "name": "wif",
  "version": "1.0.0",
  "description": "Express 5 application with Slack webhook endpoint",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "sentry-cli sourcemaps inject ./dist && sentry-cli sourcemaps upload ./dist && find ./dist -name '*.map' -delete",
    "start": "node --import=./instrument.mjs ./dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.44",
    "@ai-sdk/openai": "^3.0.29",
    "@octokit/auth-app": "^8.2.0",
    "@octokit/rest": "^22.0.1",
    "@sentry/node": "^10.39.0",
    "@sentry/profiling-node": "^10.39.0",
    "@slack/bolt": "^4.6.0",
    "ai": "^6.0.86",
    "dotenv": "^17.3.1",
    "express": "^5.0.1",
    "octokit": "^5.0.5",
    "p-queue": "^9.1.0",
    "semver": "^7.7.4",
    "zod": "^4.3.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@sentry/cli"]
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@sentry/cli": "^3.2.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/semver": "^7.7.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
